{
  "compilerOptions": {
    "target": "ES2016",
    "lib": [
      "es5",
      "es6",
      "es7",
      "es2017"
    ],
    "module": "CommonJS",
    "moduleResolution": "node10",
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./"
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}